@model IEnumerable<Proyecto>

@if(Model /*es una instancia de una clase(en este caso de un IEnumerable<Proyecto>) que representa los datos que se van a mostrar en la vista (en )*/is not null){
<div class="conteiner">
        <div class="row rows-cols-1 row-cols-md-3 g-4">
            @foreach (var proyecto in Model)
            {
                <div class="col">
                    <div class="card h-100">
                        <img src="@proyecto.ImagenURL" class=" card-img-top" alt="">
                        <div class="card-body">
                            <h5 class="card-title">@proyecto.Titulo</h5>
                            <p class= "card-text">@proyecto.Descripcion</p>
                        </div>
                        <div class="card-footer">
                            <a class="btn btn-primary" href="@proyecto.Link" 
                            target="_blank" rel="noopener noreferrer">Visitar</a>
                        </div>
                    </div>
                </div>
            }
        </div>
    </div>
}